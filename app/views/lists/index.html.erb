<%= render "/shared/navbar" %>

<div class="container">
  <h1>My Lists</h1>
</div>

<div class="container">
  <ul>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>
    <%= cl_image_tag list.photo.key, width: 300 %>
  <% end %>
  </ul>
</div>

<%= link_to 'Create new list', new_list_path %>
